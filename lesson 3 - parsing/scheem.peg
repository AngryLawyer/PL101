start =
    expression
validchar
    = [0-9a-zA-Z_?!+\-=@#$%^&*/.]

atom =
    chars:validchar+
        { return chars.join(""); }

space =
    [ \n\t\r]

spacedAtom = 
    space* right: atom
        { return right; }
        
expression =
    spacedAtom
  / space* ['] exp: expression
        { return ['quote', exp]; }
  / space* [(] middle: expression+ space* [)]
        { return middle; }
